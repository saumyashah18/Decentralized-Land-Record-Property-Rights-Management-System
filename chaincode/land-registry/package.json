{
    "name": "land-registry-chaincode",
    "version": "1.0.0",
    "description": "Hyperledger Fabric chaincode for BhoomiSetu",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "prepublishOnly": "npm run build",
        "build": "tsc",
        "start": "fabric-chaincode-node server --chaincode-address $CHAINCODE_SERVER_ADDRESS --chaincode-id $CHAINCODE_ID"
    },
    "engineStrict": true,
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "dependencies": {
        "fabric-contract-api": "^2.4.1",
        "fabric-shim": "^2.4.1"
    },
    "devDependencies": {
        "@types/node": "^18.16.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}